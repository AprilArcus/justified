{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "globalReturn": false,
      "jsx": false,
      "experimentalObjectRestSpread": false
    }
  },
  "env": {
    "node": true
  },
  "plugins": [
    "node"
  ],
  "settings": {
    "node": {
      "tryExtensions": [".js", ".json", ".node"]
    }
  },
  "rules": {
    "node/no-missing-import": 2,
    "node/no-missing-require": 2,
    "node/no-unpublished-import": 0,
    "node/no-unpublished-require": 0,
    "node/no-unsupported-features": [2, { "version": 0.12 }],
    "no-console": 0,
    "no-unexpected-multiline": 2,
    "accessor-pairs": 2,
    "consistent-return": 2,
    "curly": 0,
    "default-case": 2,
    "dot-location": [1, "property"],
    "dot-notation": 1,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-div-regex": 1,
    "no-else-return": 1,
    "no-empty-label": 1,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": [2, { "ignore": [0, 1, 2] }],
    "no-multi-spaces": [1, { "exceptions": {
      "VariableDeclarator": true,
      "ImportDeclaration": true
    }}],
    "no-multi-str": 1,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": [2, { "props": true }],
    "no-process-env": 2,
    "no-proto": 2,
    "no-redeclare": [2, { "builtinGlobals": true }],
    "no-return-assign": [2, "always"],
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-void": 2,
    "no-warning-comments": [1, { "location": "anywhere" }],
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 0,
    "wrap-iife": [2, "inside"],
    "yoda": [2, "never", { "exceptRange": true }],
    "strict": [2, "never"],
    "init-declarations": 2,
    "no-catch-shadow": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-unused-vars": [1, { "args": "none" }],
    "no-use-before-define": 2,
    "array-bracket-spacing": [1, "never", {
      "singleValue": false,
      "objectsInArrays": false,
      "arraysInArrays": true
    }],
    "block-spacing": [1, "always"],
    "brace-style": [1, "1tbs", { "allowSingleLine": false }],
    "camelcase": 1,
    "comma-spacing": [1, { "before": false, "after": true }],
    "comma-style": [1, "last", {
      "exceptions": { "VariableDeclaration": true }
    }],
    "computed-property-spacing": [1, "never"],
    "eol-last": [2, "unix"],
    "func-names": 0,
    "func-style": [2, "expression", { "allowArrowFunctions": true }],
    "indent": [1, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": {
        "var": 2,
        "let": 2,
        "const": 3
      }
    }],
    "key-spacing": 0,
    "linebreak-style": [2, "unix"],
    "lines-around-comment": 1,
    "max-depth": 0,
    "max-len": [1, 80, 4, {
      "ignoreComments": false,
      "ignoreUrls": true,
    }],
    "max-nested-callbacks": [2, 1],
    "max-params": [2, 1],
    "max-statements": 0,
    "new-cap": 2,
    "new-parens": 1,
    "newline-after-var": 0,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-continue": 2,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-multiple-empty-lines": 1,
    "no-negated-condition": 1,
    "no-nested-ternary": 1,
    "no-new-object": 2,
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
    "no-spaced-func": 2,
    "no-ternary": 1,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [1, "always"],
    "one-var": [1, {
      "var": "always",
      "let": "never",
      "const": "never"
    }],
    "operator-assignment": [1, "always"],
    "operator-linebreak": 1,
    "padded-blocks": [1, "never"],
    "quote-props": [1, "as-needed"],
    "quotes": [1, "single"],
    "require-jsdoc": 0,
    "semi-spacing": 1,
    "semi": [1, "always"],
    "sort-vars": 0,
    "space-after-keywords": 1,
    "space-before-blocks": 1,
    "space-before-function-paren": 1,
    "space-before-keywords": 1,
    "space-in-parens": 1,
    "space-infix-ops": 1,
    "space-return-throw-case": 1,
    "space-unary-ops": 1,
    "spaced-comment": 1,
    "wrap-regex": 1,
    "arrow-body-style": [1, "as-needed"],
    "arrow-parens": [1, "as-needed"],
    "arrow-spacing": 1,
    "constructor-super": 2,
    "generator-star-spacing": [1, "after"],
    "no-arrow-condition": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-this-before-super": 2,
    "no-var": 0,
    "object-shorthand": 1,
    "prefer-arrow-callback": 0,
    "prefer-const": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "require-yield": 2
  }
}
